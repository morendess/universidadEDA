package org.mp.sesion02;

public class MatrizInteger extends MatrizGenerica<Integer> {

	/**
	 * Suma los dos valores proporcionados â€¢ @return suma de o1 y o2
	 */
	protected Integer sumar(Integer o1, Integer o2) {
		return o1 + o2;
	}

	/**
	 * Multiplica los dos valores proporcionados 
	 * @return multiplicacion de o1 y
	 * o2
	 */
	protected Integer multiplicar(Integer o1, Integer o2) {
		return o1 * o2;
	}

	/**
	 * Pone a cero
	 * @return 0
	 */
	protected Integer cero() {
		return 0;
	}

}